<!DOCTYPE html>
<html>
<head>
  <title>Testaplication</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  <%= link_to t("company"), companies_path %>
